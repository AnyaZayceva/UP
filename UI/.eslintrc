{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "no-plusplus": "off",
    "no-unused-vars":"off",
    "func-names":"off",
    "no-underscore-dangle":"off",
    "no-param-reassign":"off",
    "no-alert":"off",
    "no-undef":"off",
    "comma-dangle":"off",
    "no-shadow":"off",
    "no-restricted-syntax":"off"
  }
}